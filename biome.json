{
	"$schema": "https://biomejs.dev/schemas/2.3.8/schema.json",
	"formatter": {
		"enabled": true,
		"lineEnding": "crlf"
	},
	"linter": {
		"enabled": true
	},
	"assist": {
		"enabled": true
	},
	"vcs": {
		"enabled": true,
		"clientKind": "git",
		"useIgnoreFile": true,
		"defaultBranch": "origin/main"
	}
}
